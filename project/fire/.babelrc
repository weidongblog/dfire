{
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "chrome": 52,
                    "browsers": [
                        "last 2 versions",
                        "safari 7"
                    ]
                }
            }
        ],
        "@babel/react"
    ],
    "plugins": [
        "@babel/plugin-proposal-function-bind",
        "@babel/plugin-proposal-class-properties",
        [
            "@babel/transform-runtime",
            {
                "helpers": false
            }
        ]
    ]
}